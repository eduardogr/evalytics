
[run]
branch = True
source = codecov

[report]
exclude_lines =
    pragma: no cover
    def __repr__
    if .debug:
    raise NotImplementedError
    if __name__ == .__main__.:
ignore_errors = True
omit =
    env/*
    venv/*
    tests/*
    setup.py
    google_auth.py
    server.py
    evalytics/handlers.py